<div class="wrapper">
	<div class="container">
		<div id="three-columns" class="grid-container" style="display:block;">
			<ul class="rig columns-3">
			{{#nodes}}
				{{#display}}
					<li id="{{targetid}}_{{id}}" class="item_{{targetid}}">
						<div class="card" style="border: solid 3px #{{bordercolor}};">
							<div class="card-image">
								<img src="{{img}}" />
								<div class="card-bg">
									<div class="card-date">{{date}}</div>
									<div class="card-title">{{title}}</div>
								</div>
							</div>	
						</div>
					</li>
				{{/display}}
				{{/nodes}}
			</ul>
		</div>
		<!--/#three-columns-->
		
		
	</div>
	<!--/.container-->
</div>
<!--/.wrapper-->

